<% if(alert.message != ""){ %>
<div class="alert alert-<%= alert.status %>  alert-dismissible">
  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">
    &times;
  </button>
  <h5>
    <% if(alert.status == "success"){ %>
      <i class="icon fas fa-check"></i>
      <% } else { %>
      <i class="icon fas fa-ban"></i>
    <% } %>
    <%= alert.message %> 
  </h5>
</div>
<% } %>
