<table id="example1" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>No</th>
      <th>Type</th>
      <th>Price</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% nominals.map((nominal,i)=>{ %>
    <tr>
      <td><%= i+1 %></td>
      <td><%= nominal.typeName %></td>
      <td><%= nominal.price %></td>
      <td>
        <form
          action="/nominal/delete/<%= nominal._id %>?_method=DELETE"
          method="POST"
        >
          <a
            class="btn btn-warning btn-sm"
            href="/nominal/update/<%= nominal._id %>"
            ><i class="fas fa-pencil-alt"></i> Edit</a
          >
          <button type="submit" class="btn btn-danger btn-sm">
            <i class="fas fa-trash"></i> Delete
          </button>
        </form>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
